{
  "name": "@olafsh/olaf-sdk-js",
  "version": "0.0.1",
  "description": "OLAF SDK for JS",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "npx tsc -p config/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "npx tsc -p config/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olafsh/olaf-sdk-js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olafsh/olaf-sdk-js/issues"
  },
  "homepage": "https://github.com/olafsh/olaf-sdk-js#readme",
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
